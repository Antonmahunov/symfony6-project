{% extends 'base.html.twig' %}
{% block title %}
    {{ post.title }}
{% endblock %}
{% block content %}
    {% block body %}
        <p>Id: {{ id|upper }}</p>
        <p>Post: {{ post.title|upper }}</p>
        <p>Post: {{ post.text }}</p>
        <p>Date: {{ post.date|date('H:i:s') }}</p>
        Here is the current {{ locale }}
        {{ message }}
    {% endblock %}
{% endblock %}
